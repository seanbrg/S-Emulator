<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="48.0" prefWidth="698.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.header.HeaderController">
   <children>
      <Button fx:id="loadFileButton" mnemonicParsing="false" text="Load File...">
         <HBox.margin>
            <Insets left="15.0" right="15.0" top="10.0" />
         </HBox.margin>
      </Button>
      <Label fx:id="fileLoadedLabel" opacity="0.0" text="File loaded!" textFill="RED">
         <HBox.margin>
            <Insets top="15.0" />
         </HBox.margin>
      </Label>
      <ComboBox fx:id="selectProgram" prefWidth="150.0" promptText="Select Program">
         <HBox.margin>
            <Insets left="15.0" right="15.0" top="10.0" />
         </HBox.margin>
      </ComboBox>
      <Label alignment="CENTER" text="Program Expansion:">
         <HBox.margin>
            <Insets left="25.0" right="5.0" top="15.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="degreeLabel" text="0/0">
         <HBox.margin>
            <Insets right="5.0" top="15.0" />
         </HBox.margin>
      </Label>
      <Button fx:id="expandButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" text="+">
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
         <HBox.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </HBox.margin>
      </Button>
      <Button fx:id="collapseButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" text="-">
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
         <HBox.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
