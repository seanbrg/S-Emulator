<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>

<ScrollPane xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="client.components.dashboard.dashboardStage.DashboardStageController"
>

   <content>
      <BorderPane>

         <top>
            <fx:include fx:id="header" source="../dashboardHeader/dashboardHeader.fxml"/>
         </top>

         <center>
            <SplitPane dividerPositions="0.5">


               <SplitPane orientation="VERTICAL" dividerPositions="0.5">
                  <items>
                     <fx:include fx:id="availableUsers" source="../availableUsers/availableUsers.fxml"/>
                     <fx:include fx:id="userHistory" source="../userHistory/userHistory.fxml"/>

                  </items>
               </SplitPane>


               <SplitPane orientation="VERTICAL" dividerPositions="0.5">
                  <items>
                     <fx:include fx:id="availablePrograms" source="../availablePrograms/availablePrograms.fxml"/>
                     <fx:include fx:id="availableFunctions" source="../availableFunctions/availableFunctions.fxml"/>
                  </items>
               </SplitPane>

            </SplitPane>
         </center>

      </BorderPane>
   </content>
</ScrollPane>